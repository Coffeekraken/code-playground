{
  "name": "markdown-doc-ui",
  "version": "0.0.1",
  "description": "Provide a nice standelone (jsfiddle/codepen/jsbin) like interface wrapped in a simple to use node server.",
  "homepage": "https://buzzbrothers.ch",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/code-playground"
  },
  "main": "index.js",
  "bin": {
    "coffeekraken-code-playground": "./run"
  },
  "scripts": {
    "assets": "npm run assets.build",
    "assets.build": "npm run assets.build.css && npm run assets.build.js && npm run assets.build.img",
    "assets.build.css": "node-sass public/assets-src/sass/style.scss public/assets/css/style.css --include-path node_modules --output-style compact",
    "assets.build.js": "webpack",
    "assets.build.img": "imagemin public/assets-src/img/* --out-dir=public/assets/img",
    "assets.watch": "concurrently --kill-others \"npm run assets.watch.css\" \"npm run assets.watch.js\"",
    "assets.watch.css": "nodemon -w public/assets-src -e scss -x \"npm run assets.build.css\"",
    "assets.watch.js": "nodemon -w public/assets-src -e js -x \"npm run assets.build.js\"",
    "compile-server": "coffeekraken-compile-server",
    "test": "coffeekraken-testing-stack"
  },
  "keywords": [
    "markdown",
    "documentation",
    "interface",
    "web",
    "node",
    "js",
    "npm",
    "express",
    "doc",
    "customize",
    "sugar",
    "coffeekraken",
    "coffee",
    "kraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "codemirror": "^5.22.0",
    "coffeekraken-gridle": "git+ssh://git@github.com/Coffeekraken/gridle.git#release/3.0.0",
    "coffeekraken-compile-server": "git+ssh://git@github.com/Coffeekraken/compile-server.git#release/0.0.1",
    "coffeekraken-sugar": "git+ssh://git@github.com/Coffeekraken/sugar.git#release/0.0.1",
    "coffeekraken-s-interactive-demo-component": "git+https://git@github.com/Coffeekraken/s-interactive-demo-component.git#release/0.0.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "golden-layout": "^1.5.7",
    "imagemin": "^5.2.2",
    "lodash": "^4.17.2"

  }
}
