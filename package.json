{
  "name": "coffeekraken-code-playground",
  "version": "0.0.1",
  "description": "Provide a nice code playground that let you play with some html, javascript (coffee, typescript, etc...) and css (sass, scss, stylus, etc...)",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/code-playground"
  },
  "main": "index.js",
  "bin": {
    "coffeekraken-code-playground": "./bin/coffeekraken-code-playground"
  },
  "scripts": {
    "assets": "npm run assets.css && npm run assets.js && npm run assets.img",
    "assets.css": "node-sass public/assets-src/sass/style.scss public/assets/css/style.css --include-path node_modules --output-style compact",
    "assets.js": "webpack",
    "assets.img": "imagemin public/assets-src/img/* --out-dir=public/assets/img",
    "assets.watch": "concurrently --kill-others \"npm run assets.watch.css\" \"npm run assets.watch.js\"",
    "assets.watch.css": "nodemon -w public/assets-src -e scss -x \"npm run assets.css\"",
    "assets.watch.js": "nodemon -w public/assets-src -e js -x \"npm run assets.js\"",
    "test": "coffeekraken-testing-stack",
    "demo": "./bin/coffeekraken-code-playground"
  },
  "keywords": [
    "code",
    "playground",
    "interface",
    "web",
    "node",
    "js",
    "npm",
    "express",
    "doc",
    "customize",
    "sugar",
    "coffeekraken",
    "coffee",
    "kraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "MD5": "^1.3.0",
    "codemirror": "^5.22.0",
    "coffeekraken-compile-server": "git+ssh://git@github.com/Coffeekraken/compile-server.git#release/0.0.1",
    "coffeekraken-gridle": "git+ssh://git@github.com/Coffeekraken/gridle.git#release/3.0.0",
    "coffeekraken-s-atv-card-component": "git+https://git@github.com/Coffeekraken/s-atv-card-component.git#release/0.0.1",
    "coffeekraken-s-share-component": "git+https://git@github.com/Coffeekraken/s-share-component.git#release/0.0.1",
    "coffeekraken-s-dialog-component": "git+https://git@github.com/Coffeekraken/s-dialog-component.git#release/0.0.1",
    "coffeekraken-s-interactive-demo-component": "git+https://git@github.com/Coffeekraken/s-interactive-demo-component.git#release/0.0.1",
    "coffeekraken-sugar": "git+ssh://git@github.com/Coffeekraken/sugar.git#release/0.0.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "golden-layout": "^1.5.7",
    "imagemin-cli": "^3.0.0",
    "lodash": "^4.17.2",
    "node-parallel": "^0.1.6",
    "webcomponents.js": "^0.7.23"
  }
}
